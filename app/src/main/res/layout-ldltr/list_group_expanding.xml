                        android:textAppearance="@style/IconFonts4"
                        android:textColor="@color/gray_dark"
                        android:textSize="14dp" />

                    <View
                        android:layout_width="@dimen/_100dpflight"
                        android:layout_height="@dimen/_1dp_width"
                        android:background="@color/gray_dark" />

                    <TextView
                        android:id="@+id/tvPlaneIcon2"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="2dp"
                        android:layout_marginRight="2dp"
                        android:gravity="right|center"
                        android:rotation="180"
                        android:text="@string/plane"
                        android:textAppearance="@style/IconFonts"
                        android:textColor="@color/gray_dark"
                        android:textSize="20dp" />

                    <View
                        android:id="@+id/viewLine2"
                        android:layout_width="@dimen/_100dpflight"
                        android:layout_height="@dimen/_1dp_width"
                        android:background="@color/gray_dark" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:text="@string/empty_circle"
                        android:textAppearance="@style/IconFonts4"
                        android:textColor="@color/gray_dark"
                        android:textSize="14dp" />


                </LinearLayout>

                <LinearLayout
                    android:id="@+id/linearBargashtTree"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="3dp">


                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"

                        android:layout_weight="2"
                        android:gravity="left">

                        <TextView
                            android:id="@+id/txtArrivelFalseLast"
                            android:layout_width="wrap_content"

                            android:layout_height="match_parent"
                            android:layout_marginLeft="@dimen/_10dpTabletSize"

                            android:gravity="left"
                            android:text="استانبول"
                            android:textColor="#000"
                            android:textSize="@dimen/_12dp_normal_textSize" />


                    </LinearLayout>

                    <TextView
                        android:id="@+id/lblFlightArrivalTimeB"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_gravity="center|center_vertical"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:layout_marginLeft="@dimen/_10dpTabletSize"
                        android:text="بدون توقف  "
                        android:textColor="#aaa"
                        android:textSize="@dimen/_10dp_textSize" />

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"

                        android:layout_weight="2"
                        android:gravity="right">

                        <TextView
                            android:id="@+id/txtDepurtureFalseOne"
                            android:layout_width="wrap_content"

                            android:layout_height="match_parent"
                            android:layout_marginLeft="@dimen/_10dpTabletSize"

                            android:gravity="right"
                            android:text="استانبول"
                            android:textColor="#000"
                            android:textSize="@dimen/_12dp_normal_textSize" />


                    </LinearLayout>
                </LinearLayout>
                <!-- ///////////////////////////////////////////////////// -->
            </LinearLayout>

            <RelativeLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_marginLeft="9dp"
                android:layout_marginRight="5dp"
                android:layout_weight="5"

                >

                <TextView
                    android:id="@+id/txtPin"
                    android:layout_width="18dp"
                    android:layout_height="18dp"
                    android:layout_alignParentRight="true"
                    android:layout_alignParentTop="true"
                    android:gravity="center"
                    android:text="@string/icon_pin"
                    android:textAppearance="@style/IconFonts"

                    android:textColor="#aaaaaa"
                    android:textSize="12sp"
                    android:visibility="invisible" />
                <!-- <RelativeLayout
                     android:id="@+id/RelativeLayoutHotel"
                     android:layout_width="40dp"
                     android:layout_height="40dp"

                     android:layout_toLeftOf="@+id/txtPin"
                     android:layout_toStartOf="@+id/txtPin"
                     >-->

                <com.wang.avi.AVLoadingIndicatorView

                    android:id="@+id/avi"
                    style="@style/AVLoadingIndicatorView"
                    android:layout_width="@dimen/_40dp_f"
                    android:layout_height="@dimen/_40dp_f"
                    android:layout_centerInParent="true"
                    android:visibility="visible"
                    app:indicatorColor="@color/app_base_color"
                    app:indicatorName="BallSpinFadeLoaderIndicator" />

                <ImageView
                    android:id="@+id/lblProductrow"
                    android:layout_width="@dimen/_40dp_f"
                    android:layout_height="@dimen/_40dp_f"


                    android:layout_centerInParent="true"

                    android:adjustViewBounds="true"
                    android:scaleType="fitXY"

                    />
                <!--</RelativeLayout>-->
            </RelativeLayout>
        </LinearLayout>
        <!-- ////////////////////////////front bottom///////////////////////////////////// -->

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/_35dp"
            android:layout_gravity="center"
            android:layout_marginBottom="4dp"
            android:layout_marginTop="4dp"
            android:background="@color/sorati"
            android:orientation="horizontal"
            android:padding="@dimen/_5dp_margin">


            <TextView
                android:id="@+id/btnExpand"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_marginLeft="@dimen/_5dpTabletSize"
                android:layout_weight="0.04"
                android:gravity="center"
                android:text="@string/icon_exp_up"
                android:textAppearance="@style/Arrow"
                android:textColor="#000"
                android:textSize="@dimen/text_size_12dp" />

            <TextView
                android:id="@+id/txt_economi"
                android:layout_width="0dp"
                android:layout_height="match_parent"

                android:layout_gravity="center"
                android:layout_marginLeft="@dimen/_5dp_margin_f_g"

                android:layout_weight="0.2"
                android:background="@drawable/background_strock_blue_dark_economi"
                android:gravity="center"
                android:text="@string/Economi"
                android:textColor="#003399"
                android:textSize="@dimen/_10dp_textSize" />


            <TextView
                android:id="@+id/txttedad"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_weight="0.5"

                android:gravity="center"
                android:text="فقط 4 نفر"
                android:textColor="#0e874e"
                android:textSize="@dimen/_10dp_textSize"
                android:visibility="visible" />




            <TextView
                android:id="@+id/lblAdlCost"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="5dp"
                android:layout_marginLeft="@dimen/_10dpTabletSize"
                android:gravity="right"
                android:text="10,000,000"
                android:textAppearance="@style/IrsansBold"
                android:textColor="#000000"
                android:textSize="@dimen/_13dp_normal_textSize"
                android:textStyle="bold" />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="#f5eeff"
                android:gravity="right"
                android:text="@string/Rial"
                android:textAppearance="@style/IrsansBold"
                android:layout_marginRight="@dimen/_10dpTabletSize"
                android:textColor="#0e874e"
                android:textSize="@dimen/_13dp_normal_textSize" />
