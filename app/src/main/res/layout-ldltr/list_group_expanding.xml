                    </LinearLayout>

                    <TextView

                        android:id="@+id/lblFlightArrivalTimeLongB"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_gravity="center|center_vertical"
                        android:layout_marginLeft="@dimen/_10dpTabletSize"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:text="1h 55m"
                        android:textAppearance="@style/tahoma"
                        android:textColor="#aaa"
                        android:textSize="@dimen/_12dp_normal_textSize" />

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_alignParentBottom="true"
                        android:layout_alignParentEnd="true"
                        android:layout_alignParentRight="true"
                        android:layout_gravity="right|center_vertical"
                        android:layout_weight="2"
                        android:gravity="right">

                        <TextView

                            android:id="@+id/lblArrivalCityNameFaBTime"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentBottom="true"
                            android:layout_alignParentEnd="true"
                            android:layout_alignParentRight="true"
                            android:layout_gravity="right|center_vertical"
                            android:gravity="right"
                            android:text="21:05"
                            android:textAppearance="@style/tahoma"
                            android:textColor="#003399"
                            android:textSize="@dimen/_14dp_txtSize"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/lblArrivalCityNameFaB"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
