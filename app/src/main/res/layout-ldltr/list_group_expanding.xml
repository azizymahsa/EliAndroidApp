                        android:layout_height="wrap_content"
                        android:layout_weight="2"
                        android:gravity="left">

                        <TextView
                            android:id="@+id/txtArrivelTrueLast"
                            android:layout_width="90dp"

                            android:layout_height="match_parent"
                            android:layout_marginLeft="@dimen/_10dpTabletSize"

                            android:gravity="left"
                            android:text="تهران"
                            android:textColor="#000"
                            android:textSize="@dimen/_12dp_normal_textSize" />


                    </LinearLayout>

                    <TextView

                        android:id="@+id/lblFlightArrivalTimeR"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_gravity="center|center_vertical"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:layout_marginLeft="@dimen/_10dpTabletSize"
                        android:text="بدون توقف"
                        android:textColor="#aaa"
                        android:textSize="@dimen/_10dp_textSize" />

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_alignParentBottom="true"
                        android:layout_alignParentEnd="true"
                        android:layout_alignParentRight="true"
                        android:layout_gravity="right|center_vertical"
                        android:layout_weight="2"
                        android:gravity="right">

                        <TextView
                            android:id="@+id/txtDepurtureTrueOne"
