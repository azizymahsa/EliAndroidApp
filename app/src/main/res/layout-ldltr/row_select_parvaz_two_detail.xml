<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/row"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="#fff"
    android:clickable="true">
    <!-- <LinearLayout
        style="@style/MyListFrontContentRe"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal" > -->

    <LinearLayout
        android:id="@+id/llCounter"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/back_linear_row"
        android:orientation="vertical">

        <LinearLayout
            android:id="@+id/fronttop"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_68dp_f"
            android:layout_marginTop="@dimen/_10dp_margin"
            android:layout_marginBottom="@dimen/_5dp_no_tablet"
            android:background="#ffffff"
            android:gravity="center"
            android:minHeight="@dimen/_68dp_f"
            android:orientation="horizontal"
            android:paddingLeft="@dimen/_10dpTabletSize"
            android:tag="front">

            <LinearLayout
                android:id="@+id/linearLayout2"
                android:layout_width="0dp"
                android:layout_height="@dimen/_68dp_f"
                android:layout_above="@+id/linearLayout1"
                android:layout_gravity="left|top"
                android:layout_weight="10"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/lblFlightTimeR"
                    android:layout_width="wrap_content"
                    android:layout_height="0dp"
                    android:layout_weight="350"
                    android:gravity="center|top"
                    android:paddingTop="3dp"
                    android:text="11:40 AM"
                    android:textAppearance="@style/fontello_plan"
                    android:textColor="#003399"
                    android:textSize="@dimen/_12dp_normal_textSize" />

                <TextView

                    android:layout_width="wrap_content"
                    android:layout_height="0dp"
                    android:layout_weight="450"
                    android:gravity="right"
                    android:text=" 11:50 AM "
                    android:textColor="#486b6b"
                    android:textSize="12dp"
                    android:visibility="invisible" />

                <TextView
                    android:id="@+id/lblFlightArrivalTimeR"
                    android:layout_width="wrap_content"
                    android:layout_height="0dp"
                    android:layout_weight="350"
                    android:gravity="center|bottom"
                    android:paddingBottom="3dp"
                    android:text="11:40 AM"
                    android:textAppearance="@style/fontello_plan"
                    android:textColor="#003399"
                    android:textSize="@dimen/_12dp_normal_textSize" />

            </LinearLayout>

            <ImageView
                android:id="@+id/s1"
                android:layout_width="@dimen/_8dp_f"
                android:layout_height="match_parent"
                android:layout_marginLeft="2dp"
                android:layout_marginRight="2dp"
                android:gravity="right"
                android:paddingBottom="3dp"
                android:paddingTop="3dp"
                android:scaleType="fitXY"
                android:src="@drawable/blb" />


            <LinearLayout
                android:id="@+id/linearLayout1"
                android:layout_width="0dip"
                android:layout_height="match_parent"
                android:layout_gravity="left"
                android:layout_weight="80"
                android:gravity="left|top"
                android:orientation="vertical"
                android:paddingLeft="@dimen/_10dpTabletSize">


                <TextView
                    android:id="@+id/lblDepurtureAirportR"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="350"
                    android:ellipsize="end"
                    android:gravity="left|top"
                    android:singleLine="true"
                    android:text="استانبول"
                    android:textColor="#003399"
                    android:textSize="@dimen/_12dp_normal_textSize" />

                <LinearLayout

                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="left"
                    android:layout_marginLeft="3dip"
                    android:layout_marginTop="4dp"
                    android:layout_weight="50"
                    android:gravity="left|top"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/lblFlightNumberR"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="50"
                        android:gravity="left|center"
                        android:paddingTop="2dp"
                        android:text="TK658 "
                        android:textAppearance="@style/tahoma"
                        android:textAllCaps="false"
                        android:textColor="#0e874e"
                        android:textSize="@dimen/_12dp_normal_textSize" />

                    <TextView
                        android:id="@+id/lblFlightNumberPersian"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="50"
                        android:gravity="left|center"
                        android:paddingTop="2dp"
                        android:text="TK658"
                        android:textAllCaps="false"
                        android:textColor="#0e874e"
                        android:textSize="@dimen/_12dp_normal_textSize" />
                </LinearLayout>

                <TextView
                    android:id="@+id/lblArrivalAirportR"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_marginLeft="3dip"
                    android:layout_marginTop="4dp"
                    android:layout_weight="350"
                    android:ellipsize="end"
                    android:gravity="left|bottom"
                    android:singleLine="true"
                    android:text="استانبول"
                    android:textColor="#003399"
                    android:textSize="@dimen/_12dp_normal_textSize" />

            </LinearLayout>

        </LinearLayout>
        <!--   ////////////////////////////weight///////////////////////////////////// -->
        <LinearLayout
            android:id="@+id/weightLinear"
            android:layout_width="match_parent"
            android:layout_height="22dp"

            android:background="#ffffff"
            android:gravity="center"
            android:minHeight="@dimen/_68dp_f"
            android:orientation="horizontal"
            android:paddingLeft="@dimen/_10dpTabletSize"
            android:layout_marginBottom="@dimen/_5dp_flight_weight">

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="@dimen/_68dp_f"
                android:layout_above="@+id/linearLayout1"
                android:layout_gravity="left|top"
                android:layout_weight="10"
                android:orientation="vertical">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="0dp"
                    android:layout_weight="350"
                    android:gravity="center|top"
                    android:paddingTop="3dp"
                    android:text="11:40 AM"
                    android:textAppearance="@style/fontello_plan"
                    android:textColor="#003399"
                    android:visibility="invisible"
                    android:textSize="@dimen/_12dp_normal_textSize" />

            </LinearLayout>

            <ImageView
                android:visibility="invisible"
                android:layout_width="@dimen/_8dp_f"
                android:layout_height="match_parent"
                android:layout_marginLeft="2dp"
                android:layout_marginRight="2dp"
                android:gravity="right"
                android:paddingBottom="3dp"
                android:paddingTop="3dp"
                android:scaleType="fitXY"
                android:src="@drawable/blb" />


            <LinearLayout
                android:id="@+id/linearWeight"
                android:layout_width="0dip"
                android:layout_height="match_parent"
                android:layout_gravity="left"

                android:layout_weight="80"
                android:gravity="left|top"
                android:orientation="horizontal"
                android:paddingLeft="@dimen/_10dpTabletSize"
                >


                <TextView
                    android:id="@+id/txtPieces"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="3dip"
                    android:layout_marginRight="2dp"
                    android:gravity="left|center"

                    android:text="1×"
                    android:textAppearance="@style/IconFonts"
                    android:textColor="#003399"
                    android:textSize="@dimen/_12dp_normal_textSize" />

                <TextView
                    android:id="@+id/iconPieces"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="2dp"
                    android:layout_marginRight="2dp"
                    android:layout_marginTop="3dp"
                    android:gravity="left|center"
                    android:text="@string/Pieces"
                    android:textAppearance="@style/fontello_plan"
                    android:textColor="@color/light_orang"
                    android:textSize="@dimen/_14dp_txtSize" />

                <TextView
                    android:id="@+id/txtWeight"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="4dp"
                    android:layout_marginRight="2dp"
                    android:gravity="left|center"

                    android:text="30"
                    android:textAppearance="@style/IconFonts"
                    android:textColor="#003399"
                    android:textSize="@dimen/_12dp_normal_textSize" />

                <TextView
                    android:id="@+id/iconWeight"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="2dp"
                    android:layout_marginRight="2dp"
                    android:gravity="left|center"
                    android:text="@string/weight"
                    android:textAppearance="@style/fontello_plan"
                    android:textColor="@color/light_orang"
                    android:textSize="@dimen/_16dp_normal_textSize" />

            </LinearLayout>

        </LinearLayout>

        <!--   ////////////////////////////front bottom///////////////////////////////////// -->


        <LinearLayout
            android:id="@+id/linearTableNerkh"
            android:layout_marginTop="@dimen/_10dp_margin"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:visibility="visible">

            <TextView
                android:id="@+id/editText1"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_30dp_flight"
                android:layout_gravity="center|center_vertical"
                android:layout_marginBottom="@dimen/_1dp_flight"
                android:layout_marginRight="2dp"
                android:layout_marginTop="@dimen/_1dp_flight"
                android:background="#d5d9dc"
                android:gravity="left|center_vertical"
                android:paddingLeft="@dimen/_10dpTabletSize"
                android:paddingRight="@dimen/_10dpTabletSize"
                android:text="@string/costs_table"
                android:textColor="#595959"
                android:textSize="@dimen/_14dp_txtSize"
                android:textStyle="bold" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:layout_marginBottom="@dimen/_1dp_flight"
                android:layout_marginLeft="@dimen/_10dpTabletSize"
                android:layout_marginRight="@dimen/_10dpTabletSize">

                <TextView
                    android:id="@+id/textView3"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginRight="2dp"
                    android:layout_weight="50"

                    android:text="@string/base_price"

                    android:textColor="#aaaaaa"
                    android:textSize="@dimen/_13dp_normal_textSize" />

                <TextView
                    android:id="@+id/txtAdlCostP"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_weight="50"
                    android:gravity="right"
                    android:text="12,000,000"
                    android:textColor="#000000"
                    android:textSize="@dimen/_13dp_normal_textSize"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/txtAdlCostPR"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginRight="2dp"
                    android:layout_weight="8"

                    android:gravity="right"
                    android:text="@string/curency"

                    android:textColor="#24b574"
                    android:textSize="@dimen/_13dp_normal_textSize" />

            </LinearLayout>

            <View
                android:layout_width="fill_parent"
                android:layout_height="1dp"
                android:background="#e6e6e6" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:layout_marginBottom="@dimen/_1dp_flight"
                android:layout_marginLeft="@dimen/_10dpTabletSize"
                android:layout_marginRight="@dimen/_10dpTabletSize"
                android:layout_marginTop="@dimen/_1dp_flight">

                <TextView
                    android:id="@+id/textView5"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginBottom="@dimen/_1dp_width"
                    android:layout_marginLeft="2dp"
                    android:layout_marginTop="@dimen/_1dp_width"
                    android:layout_weight="50"
                    android:gravity="left"
                    android:text="@string/tax"
                    android:textColor="#aaaaaa"
                    android:textSize="@dimen/_12dp_normal_textSize" />

                <TextView
                    android:id="@+id/txtTaxes"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_weight="50"
                    android:gravity="right"
                    android:text="12,000,000"
                    android:textColor="#000000"
                    android:textSize="@dimen/_13dp_normal_textSize"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/txtTaxesR"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginRight="2dp"
                    android:layout_weight="8"

                    android:gravity="right"
                    android:text="@string/curency"

                    android:textColor="#24b574"
                    android:textSize="@dimen/_13dp_normal_textSize" />


            </LinearLayout>

            <View
                android:layout_width="fill_parent"
                android:layout_height="1dp"
                android:background="#e6e6e6" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:layout_marginBottom="@dimen/_1dp_flight"
                android:layout_marginLeft="@dimen/_10dpTabletSize"
                android:layout_marginRight="@dimen/_10dpTabletSize"
                android:layout_marginTop="@dimen/_1dp_flight">

                <TextView
                    android:id="@+id/textView7"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginRight="2dp"
                    android:layout_weight="50"
                    android:gravity="left"
                    android:text="@string/payable"
                    android:textColor="#000000"
                    android:textSize="@dimen/_13dp_normal_textSize"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/txtTotalFareCost"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_weight="50"
                    android:gravity="right"
                    android:text="12,000,000"
                    android:textColor="#000000"
                    android:textSize="@dimen/_13dp_normal_textSize"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/txtTotalFareCostR"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginRight="2dp"
                    android:layout_weight="8"

                    android:gravity="right"
                    android:text="@string/curency"

                    android:textColor="#24b574"
                    android:textSize="@dimen/_13dp_normal_textSize" />


            </LinearLayout>

            <View
                android:layout_width="fill_parent"
                android:layout_height="1dp"
                android:background="#e6e6e6" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/linearButton"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#fff"
            android:orientation="vertical"
            android:visibility="visible">

            <Button
                android:id="@+id/btnSelect"
                android:layout_width="@dimen/_123dp"
                android:layout_height="@dimen/_28dp_f"
                android:layout_marginBottom="10dp"
                android:layout_marginLeft="@dimen/_10dpTabletSize"
                android:layout_marginRight="14dp"
                android:layout_marginTop="10dp"
                android:background="@drawable/btn_plan_select"
                android:text="@string/select_and_reserve"
                android:textColor="#fff"
                android:textSize="@dimen/_12dp_normal_textSize"
                android:visibility="visible"
                />

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@android:color/darker_gray" />
        </LinearLayout>


    </LinearLayout>
</FrameLayout>