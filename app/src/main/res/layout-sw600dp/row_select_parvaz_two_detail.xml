<?xml version="1.0" encoding="utf-8"?>
 <FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:clickable="true"
    android:id="@+id/row" >

        <LinearLayout
            android:id="@+id/back"
            style="@style/MyListBackContent"
            android:layout_height="182dp"
            android:orientation="horizontal"
            android:tag="back" >

            <Button
                android:id="@+id/swipe_button"
                style="@style/MyListButtonAction"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="0.94"
                android:background="#ffffff"
                android:text=" ssssssssssssss"
                android:textColor="#000000"
                android:textSize="20dp" />

        </LinearLayout>
    <!-- <LinearLayout
        style="@style/MyListFrontContentRe"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal" > -->

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="200dp"
            android:orientation="vertical"
            android:background="@drawable/back_linear_row" >
	    
         <LinearLayout
        android:id="@+id/fronttop"
        style="@style/MyListFrontContentRe"
        android:layout_width="match_parent"
        android:layout_height="80dp"
        android:gravity="center"
        android:minHeight="40dp"
        android:background="#ffffff"
        android:orientation="horizontal"
        android:tag="front" >

	   <!--  <ImageView 
	        android:layout_width="wrap_content"
	        android:layout_height="match_parent"
	        android:layout_gravity="center_vertical"
	        
	        /> -->

	    <LinearLayout
            android:id="@+id/linearLayout1"
            android:layout_width="0dip"
            android:layout_height="match_parent"
            android:layout_gravity="right"
            android:layout_marginRight="3dip"
            android:layout_weight="0.83"
            android:gravity="right"
            android:orientation="vertical" >

	       
		  
		    <TextView 
		      
		        android:id="@+id/lblDepurtureAirportR"
		        android:layout_width="match_parent"
		        android:layout_height="wrap_content"
		        
		        android:textColor="#000"
		        android:gravity="right"
		        android:textSize="19dip"
		        android:text="استانبول"
		        />

	         <TextView
	             android:id="@+id/lblFlightNumberR"
	             android:layout_width="match_parent"
	             android:layout_height="wrap_content"
	             android:gravity="right"
	             android:text="3h 45 m ,ترکیش ایر "
	             android:textColor="#486b6b"
	             android:textSize="12dip" />
	         
	          <TextView 
		        android:layout_width="match_parent"
		        android:layout_height="wrap_content"
		        android:id="@+id/lblArrivalAirportR"
		        android:textColor="#000"
		        android:gravity="right"
		        android:textSize="19dip"
		        android:text="استانبول"
		        />
	    </LinearLayout>
	     <LinearLayout
    
      android:layout_width="wrap_content"
      android:layout_height="match_parent"
      android:layout_above="@+id/linearLayout1"
      android:layout_gravity="right"
      android:layout_marginRight="3dip"
      android:gravity="right"
      android:orientation="vertical" >

       <TextView
           android:id="@+id/s1"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:layout_margin="2dp"
           android:background="@drawable/blb"
           android:gravity="right"
           android:text=""
           android:textColor="#486b6b"
           android:textSize="12dip" />

  </LinearLayout>
	    
	     <LinearLayout
	         android:id="@+id/linearLayout2"
	         android:layout_width="wrap_content"
	         android:layout_height="match_parent"
	         android:layout_above="@+id/linearLayout1"
	         android:layout_gravity="right"
	         android:layout_marginRight="3dip"
	         android:layout_weight="0.06"
	         android:gravity="right"
	         android:orientation="vertical" >
	     
		    <TextView
		        android:id="@+id/lblFlightTimeR"
		        android:layout_width="match_parent"
		        android:layout_height="wrap_content"
		        android:layout_gravity="center"
		        android:gravity="center"
		        android:text="11:40 AM"
		        android:textColor="#000"
		        android:textSize="16dip" />

	         <TextView
	             
	             android:layout_width="match_parent"
	             android:layout_height="wrap_content"
	             android:gravity="right"
	             android:text=" 11:50 AM "
	             android:textColor="#486b6b"
	             android:textSize="12dip"
	             android:visibility="invisible" />
	         
	          <TextView
	              android:id="@+id/lblFlightArrivalTimeR"
	              android:layout_width="match_parent"
	              android:layout_height="wrap_content"
	              android:gravity="center"
	              android:text="2:40 AM"
	              android:textColor="#000"
	              android:textSize="16dip" />
	         
	    </LinearLayout>
	      
    </LinearLayout>
  <!--   ////////////////////////////front bottom///////////////////////////////////// -->
  <LinearLayout
        android:id="@+id/frontbottom"
        style="@style/MyListFrontContentRe"
        android:layout_width="match_parent"
        android:layout_height="80dp"
        android:gravity="center"
        android:minHeight="40dp"
        android:background="#ffffff"
        android:orientation="horizontal"
        android:tag="front" >

	    <LinearLayout
            android:id="@+id/linearLayout1w"
            android:layout_width="0dip"
            android:layout_height="match_parent"
            android:layout_gravity="right"
            android:layout_marginRight="3dip"
            android:layout_weight="0.83"
            android:gravity="right"
            android:orientation="vertical" >

	       
		    <TextView 
		      
		        android:id="@+id/lblDepurtureAirportB"
		        android:layout_width="match_parent"
		        android:layout_height="wrap_content"
		      
		        android:textColor="#000"
		        android:gravity="right"
		        android:textSize="19dip"
		        android:text="استانبول"
		        />

	         <TextView
	             android:id="@+id/lblFlightNumberB"
	             android:layout_width="match_parent"
	             android:layout_height="wrap_content"
	             android:gravity="right"
	             android:text="3h 45 m ,ترکیش ایر "
	             android:textColor="#486b6b"
	             android:textSize="12dip" />
	         
	          <TextView 
		        android:layout_width="match_parent"
		        android:layout_height="wrap_content"
		        android:id="@+id/lblArrivalAirportB"
		        android:textColor="#000"
		        android:gravity="right"
		        android:textSize="19dip"
		        android:text="استانبول"
		        />
		    
	         
	    </LinearLayout>

  <LinearLayout
    
      android:layout_width="wrap_content"
      android:layout_height="match_parent"
      android:layout_above="@+id/linearLayout1"
      android:layout_gravity="right"
      android:layout_marginRight="3dip"
      android:gravity="right"
      android:orientation="vertical" >

       <TextView
           android:id="@+id/ss"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:layout_margin="2dp"
           android:background="@drawable/blb"
           android:gravity="right"
           android:text=""
           android:textColor="#486b6b"
           android:textSize="12dip" />

  </LinearLayout>

	     <LinearLayout
	         android:id="@+id/linearLayout2w"
	         android:layout_width="wrap_content"
	         android:layout_height="match_parent"
	         android:layout_above="@+id/linearLayout1"
	         android:layout_gravity="right"
	         android:layout_marginRight="3dip"
	         android:layout_weight="0.06"
	         android:gravity="right"
	         android:orientation="vertical" >
	       
		    <TextView 
		        android:layout_width="match_parent"
		        android:layout_height="wrap_content"
		        android:id="@+id/lblFlightTimeB"
		        android:textColor="#000"
		        android:gravity="center"
		        android:textSize="16dip"
		        android:text="11:40 AM"
		        />

	         <TextView
	             android:id="@+id/gg"
	             android:layout_width="match_parent"
	             android:layout_height="wrap_content"
	             android:gravity="right"
	             android:text=" 11:50 AM "
	             android:textColor="#486b6b"
	             android:textSize="12dip"
	             android:visibility="invisible" />
	         
	          <TextView 
		        android:layout_width="match_parent"
		        android:layout_height="wrap_content"
		        android:id="@+id/lblFlightArrivalTimeB"
		        android:textColor="#000"
		        android:gravity="center"
		        android:textSize="16dip"
		        android:text="2:40 AM"
		        />
	         
	    </LinearLayout>  
	    
	      
    </LinearLayout>

  <LinearLayout
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:orientation="vertical" >

      <Button
          android:id="@+id/btnSelect"
          style="?android:attr/buttonStyleSmall"
          android:layout_width="123dp"
          android:layout_height="wrap_content"
          android:layout_margin="5dp"
          android:layout_marginLeft="14dp"
          android:layout_marginRight="14dp"
          android:textColor="#fff"
          android:background="@drawable/btn_plan_select"
          android:text="انتخاب و رزرو" />

  </LinearLayout>

     </LinearLayout>
</FrameLayout>    