<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="#ffffff"
    android:orientation="vertical"
    android:padding="1dp">

    <!--
         <LinearLayout
        style="@style/MyListFrontContentRe"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal" >
    -->

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        >

        <TextView
            android:id="@+id/TextView01"
            android:layout_width="8dp"
            android:layout_height="10dp"
            android:layout_gravity="right"
            android:layout_marginBottom="1dp"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="1dp"
            android:layout_marginTop="1dp"
            android:background="@drawable/pin_icon_for_items"
            android:gravity="center"
            android:textColor="#FFFFFF"
            android:textSize="12sp"
            android:visibility="invisible"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginBottom="1dp"
        android:layout_marginLeft="1dp"
        android:layout_marginTop="1dp"
        android:layout_weight="0.02"
        android:orientation="vertical">

        <LinearLayout
            android:id="@+id/fronttop"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="1dp"
            android:layout_marginRight="1dp"
            android:background="#ffffff"
            android:gravity="center"
            android:minHeight="40dp"
            android:orientation="horizontal">

            <!--
             <ImageView
	        android:layout_width="wrap_content"
	        android:layout_height="match_parent"
	        android:layout_gravity="center_vertical"

	        />
            -->

            <LinearLayout
                android:layout_width="0dp"
                android:layout_weight="20"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <!-- <TextView
                         android:id="@+id/textView1"
                        android:layout_width="0dp"
                         android:layout_weight="1"
                         android:layout_height="wrap_content"
                           android:gravity="right"
                         android:text="TextView" /> -->
                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="3"
                        android:gravity="left">

                        <TextView
                            android:id="@+id/num_flight_r"
                            android:layout_width="90dp"

                            android:layout_height="match_parent"
                            android:layout_marginLeft="1dp"
                            android:textAppearance="@style/IrsansBold"
                            android:gravity="center"
                            android:text="TK898"
                            android:textColor="#003399"
                            android:textSize="@dimen/_12dp_txtSize" />


                    </LinearLayout>


                    <TextView

                        android:id="@+id/lblFlightArrivalTimeR"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_gravity="center|center_vertical"
                        android:layout_weight="1"


                        android:text="6"
                        android:textColor="#003399"
                        android:textSize="@dimen/_12dp_txtSize" />

                    <TextView

                        android:id="@+id/lblArrivalCityNameFaR"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="2"
                        android:layout_gravity="right|center_vertical"
                        android:text="یکشنبه,21:05"
                        android:textColor="#003399"
                        android:textSize="@dimen/_12dp_txtSize"
                        android:textAppearance="@style/IrsansBold"/>

                </LinearLayout>


                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent">
                    <ImageView

                        android:layout_width="270dp"
                        android:layout_height="18dp"
                        android:layout_gravity="right"
                        android:layout_marginRight="1dp"
                        android:layout_alignParentRight="true"
                        android:src="@drawable/gray_line"
                        android:gravity="right"
                        android:text=" "
                        android:scaleType="fitXY"
                        android:textColor="#486b6b"
                        android:textSize="10dip"
                        android:visibility="visible"
                        android:padding="4dp"/>
                </RelativeLayout>



                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <!-- <TextView
                         android:id="@+id/textView1"
                        android:layout_width="0dp"
                         android:layout_weight="1"
                         android:layout_height="wrap_content"
                           android:gravity="right"
                         android:text="TextView" /> -->
                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="3"
                        android:gravity="left">

                        <TextView
                            android:id="@+id/txtArrivelTrueLast"
                            android:layout_width="90dp"

                            android:layout_height="match_parent"
                            android:layout_marginLeft="1dp"

                            android:gravity="center"
                            android:text="تهران"
                            android:textColor="#000"
                            android:textSize="@dimen/_12dp_txtSize" />


                    </LinearLayout>

                    <TextView

                        android:id="@+id/txtDepurtureTrueOne"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"

                        android:layout_weight="2"

                        android:layout_gravity="right|center_vertical"

                        android:text="استانبول  "
                        android:textColor="#000"
                        android:textSize="@dimen/_12dp_txtSize" />

                </LinearLayout>
                <!-- /////////////////////////Bargasht/////////////////////////////// -->
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="15dp">

                    <!-- <TextView
                         android:id="@+id/textView1"
                        android:layout_width="0dp"
                         android:layout_weight="1"
                         android:layout_height="wrap_content"
                           android:gravity="right"
                         android:text="TextView" /> -->
                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="3"
                        android:gravity="left">

                        <TextView
                            android:id="@+id/num_flight_b"
                            android:layout_width="90dp"

                            android:layout_height="match_parent"
                            android:layout_marginLeft="1dp"
                            android:textAppearance="@style/IrsansBold"
                            android:gravity="center"
                            android:text="TK898"
                            android:textColor="#003399"
                            android:textSize="@dimen/_12dp_txtSize" />


                    </LinearLayout>


                    <TextView

                        android:id="@+id/lblFlightArrivalTimeB"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_gravity="center|center_vertical"
                        android:layout_weight="1"


                        android:text="5"
                        android:textColor="#003399"
                        android:textSize="@dimen/_12dp_txtSize" />

                    <TextView

                        android:id="@+id/lblArrivalCityNameFaB"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"

                        android:layout_weight="2"

                        android:layout_gravity="right|center_vertical"
                        android:textAppearance="@style/IrsansBold"
                        android:text="یکشنبه,21:05"
                        android:textColor="#003399"
                        android:textSize="@dimen/_12dp_txtSize" />

                </LinearLayout>

                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent">
                    <ImageView

                        android:layout_width="270dp"
                        android:layout_height="18dp"
                        android:layout_gravity="right"
                        android:layout_marginRight="1dp"
                        android:layout_alignParentRight="true"
                        android:src="@drawable/gray_line"
                        android:gravity="right"
                        android:text=" "
                        android:scaleType="fitXY"
                        android:textColor="#486b6b"
                        android:textSize="10dip"
                        android:visibility="visible"
                        android:padding="4dp"/>
                </RelativeLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <!-- <TextView
                         android:id="@+id/textView1"
                        android:layout_width="0dp"
                         android:layout_weight="1"
                         android:layout_height="wrap_content"
                           android:gravity="right"
                         android:text="TextView" /> -->
                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="3"
                        android:gravity="left">

                        <TextView
                            android:id="@+id/txtArrivelFalseLast"
                            android:layout_width="90dp"

                            android:layout_height="match_parent"
                            android:layout_marginLeft="1dp"

                            android:gravity="center"
                            android:text="استانبول"
                            android:textColor="#000"
                            android:textSize="@dimen/_12dp_txtSize" />


                    </LinearLayout>

                    <TextView

                        android:id="@+id/txtDepurtureFalseOne"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"

                        android:layout_weight="2"

                        android:layout_gravity="right|center_vertical"

                        android:text="تهران  "
                        android:textColor="#000"
                        android:textSize="@dimen/_12dp_txtSize" />

                </LinearLayout>
                <!-- ///////////////////////////////////////////////////// -->
            </LinearLayout>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="3"
                android:layout_marginLeft="9dp"
                android:layout_marginRight="5dp"
                android:gravity="center">
                <ImageView
                    android:id="@+id/lblProductrow"
                    android:layout_width="35dp"
                    android:layout_height="35dp"
                    android:layout_alignParentRight="true"
                    android:layout_centerInParent="true"
                    android:layout_marginBottom="8dp"

                    android:layout_marginTop="2dp"
                    android:adjustViewBounds="true"
                    android:scaleType="fitXY" />
            </LinearLayout>
        </LinearLayout>
        <!-- ////////////////////////////front bottom///////////////////////////////////// -->

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            android:background="#f5eeff" >
            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="3"
                android:gravity="left">

                <TextView
                    android:id="@+id/textView1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="--"
                    android:visibility="gone"/>

                <TextView
                    android:id="@+id/txt_economi"
                    android:layout_width="55dp"

                    android:layout_height="match_parent"
                    android:layout_marginLeft="28dp"
                    android:layout_marginTop="4dp"
                    android:layout_marginBottom="4dp"
                    android:background="@drawable/background_strock_blue_dark_economi"
                    android:gravity="center"
                    android:text="اکونومی"
                    android:textColor="#003399"
                    android:textSize="@dimen/_12dp_txtSize" />

                <TextView
                    android:id="@+id/txttedad"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_weight="0.5"

                    android:gravity="center"
                    android:text="فقط 4 نفر"
                    android:textColor="#0e874e"
                    android:textSize="10dip"
                    android:visibility="visible" />
            </LinearLayout>
            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_gravity="right"
                android:layout_weight="2">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="#f5eeff"
                    android:gravity="right"
                    android:text="ریال"
                    android:textColor="#0e874e"

                    android:textAppearance="@style/IrsansBold"
                    android:textSize="17dp" />

                <TextView
                    android:id="@+id/lblAdlCost"
                    android:layout_width="120dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:textAppearance="@style/IrsansBold"
                    android:gravity="left"
                    android:text="10,000,000"
                    android:textColor="#000000"
                    android:textSize="17dp"
                    android:textStyle="bold" />
            </LinearLayout>
        </LinearLayout>

    </LinearLayout>

</LinearLayout>