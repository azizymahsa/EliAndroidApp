<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:fancy="http://schemas.android.com/apk/res-auto"
    android:background="#ffffff"
    android:orientation="vertical"
  >

    <!--
         <LinearLayout
        style="@style/MyListFrontContentRe"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal" >
    -->

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical" android:layout_marginTop="4dp"
        >

        <TextView
            android:id="@+id/TextView"
            android:layout_width="8dp"
            android:layout_height="10dp"
            android:layout_gravity="right"
            android:layout_marginBottom="1dp"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="1dp"
            android:layout_marginTop="1dp"
            android:background="@drawable/pin_icon_for_items"
            android:gravity="center"
            android:textColor="#FFFFFF"
            android:textSize="12sp"
            android:visibility="gone"/>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linear_all"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginBottom="1dp"
        android:layout_marginLeft="1dp"
        android:layout_marginTop="4dp"
        android:layout_weight="0.02"
        android:orientation="vertical">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            >

            <TextView
                android:id="@+id/TextView01"
                android:layout_width="8dp"
                android:layout_height="10dp"
                android:layout_gravity="right"
                android:layout_marginBottom="1dp"
                android:layout_marginLeft="8dp"
                android:layout_marginRight="1dp"
                android:layout_marginTop="1dp"
                android:background="@drawable/pin_icon_for_items"
                android:gravity="center"
                android:textColor="#FFFFFF"
                android:textSize="12sp"
                android:visibility="gone"/>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/fronttop"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="1dp"
            android:layout_marginRight="1dp"
            android:layout_marginBottom="8dp"
            android:background="#ffffff"
            android:gravity="center"
            android:minHeight="40dp"
            android:orientation="horizontal">

            <!--
             <ImageView
	        android:layout_width="wrap_content"
	        android:layout_height="match_parent"
	        android:layout_gravity="center_vertical"

	        />
            -->

            <LinearLayout
                android:layout_width="0dp"
                android:layout_weight="20"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="3dp">

                    <!-- <TextView
                         android:id="@+id/textView1"
                        android:layout_width="0dp"
                         android:layout_weight="1"
                         android:layout_height="wrap_content"
                           android:gravity="right"
                         android:text="TextView" /> -->
                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="2"
                        android:gravity="left">

                        <TextView
                            android:id="@+id/num_flight_r"
                            android:layout_width="90dp"

                            android:layout_height="match_parent"
                            android:layout_marginLeft="1dp"
                            android:textAppearance="@style/IrsansBold"
                            android:gravity="center"
                            android:text="TK898"
                            android:textColor="#003399"
                            android:textSize="@dimen/_12dp_txtSize" />


                    </LinearLayout>


                    <TextView

                        android:id="@+id/lblFlightArrivalTimeR"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_gravity="center|center_vertical"
                        android:layout_weight="1"
                        android:gravity="center"

                        android:text="6"
                        android:textColor="#aaa"
                        android:textSize="@dimen/_12dp_txtSize" />

                    <TextView

                        android:id="@+id/lblArrivalCityNameFaR"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_alignParentBottom="true"
                        android:layout_alignParentEnd="true"
                        android:layout_alignParentRight="true"
                        android:layout_gravity="right|center_vertical"
                        android:layout_weight="2"
                        android:text="یکشنبه,21:05"
                        android:textAppearance="@style/IrsansBold"
                        android:textColor="#003399"
                        android:textSize="@dimen/_12dp_txtSize" />

                </LinearLayout>


                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent">
                    <ImageView

                        android:layout_width="260dp"
                        android:layout_height="18dp"
                        android:layout_gravity="right"
                        android:layout_marginRight="0dp"
                        android:layout_alignParentRight="true"
                        android:src="@drawable/gray_line"
                        android:gravity="right"
                        android:text=" "
                        android:scaleType="fitXY"
                        android:textColor="#486b6b"
                        android:textSize="10dip"
                        android:visibility="visible"
                        android:padding="4dp"/>
                </RelativeLayout>



                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="3dp">

                    <!-- <TextView
                         android:id="@+id/textView1"
                        android:layout_width="0dp"
                         android:layout_weight="1"
                         android:layout_height="wrap_content"
                           android:gravity="right"
                         android:text="TextView" /> -->
                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="2"
                        android:gravity="left">

                        <TextView
                            android:id="@+id/txtArrivelTrueLast"
                            android:layout_width="90dp"

                            android:layout_height="match_parent"
                            android:layout_marginLeft="1dp"

                            android:gravity="center"
                            android:text="تهران"
                            android:textColor="#000"
                            android:textSize="@dimen/_12dp_txtSize" />


                    </LinearLayout>

                    <TextView

                        android:id="@+id/txtDepurtureTrueOne"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"

                        android:layout_weight="2"

                        android:layout_gravity="right|center_vertical"

                        android:text="استانبول  "
                        android:textColor="#000"
                        android:textSize="@dimen/_12dp_txtSize" />

                </LinearLayout>
                <!-- /////////////////////////Bargasht/////////////////////////////// -->
                <LinearLayout
                    android:id="@+id/linearBargashtOne"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="15dp"
                    android:layout_marginRight="3dp">

                    <!-- <TextView
                         android:id="@+id/textView1"
                        android:layout_width="0dp"
                         android:layout_weight="1"
                         android:layout_height="wrap_content"
                           android:gravity="right"
                         android:text="TextView" /> -->
                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="2"
                        android:gravity="left">

                        <TextView
                            android:id="@+id/num_flight_b"
                            android:layout_width="90dp"

                            android:layout_height="match_parent"
                            android:layout_marginLeft="1dp"
                            android:textAppearance="@style/IrsansBold"
                            android:gravity="center"
                            android:text="TK898"
                            android:textColor="#003399"
                            android:textSize="@dimen/_12dp_txtSize" />


                    </LinearLayout>


                    <TextView

                        android:id="@+id/lblFlightArrivalTimeB"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_gravity="center|center_vertical"
                        android:layout_weight="1"
                        android:gravity="center"

                        android:text="5"
                        android:textColor="#aaa"
                        android:textSize="@dimen/_12dp_txtSize" />

                    <TextView

                        android:id="@+id/lblArrivalCityNameFaB"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"

                        android:layout_weight="2"

                        android:layout_gravity="right|center_vertical"
                        android:textAppearance="@style/IrsansBold"
                        android:text="یکشنبه,21:05"
                        android:textColor="#003399"
                        android:textSize="@dimen/_12dp_txtSize" />

                </LinearLayout>

                <RelativeLayout
                    android:id="@+id/linearBargashtTwo"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent">
                    <ImageView

                        android:layout_width="260dp"
                        android:layout_height="18dp"
                        android:layout_gravity="right"
                        android:layout_marginRight="0dp"
                        android:layout_alignParentRight="true"
                        android:src="@drawable/gray_line"
                        android:gravity="right"
                        android:text=" "
                        android:scaleType="fitXY"
                        android:textColor="#486b6b"
                        android:textSize="10dip"
                        android:visibility="visible"
                        android:padding="4dp"/>
                </RelativeLayout>
                <LinearLayout
                    android:id="@+id/linearBargashtTree"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="3dp">


                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="2"
                        android:gravity="left">

                        <TextView
                            android:id="@+id/txtArrivelFalseLast"
                            android:layout_width="90dp"

                            android:layout_height="match_parent"
                            android:layout_marginLeft="1dp"

                            android:gravity="center"
                            android:text="استانبول"
                            android:textColor="#000"
                            android:textSize="@dimen/_12dp_txtSize" />


                    </LinearLayout>

                    <TextView

                        android:id="@+id/txtDepurtureFalseOne"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"

                        android:layout_weight="2"

                        android:layout_gravity="right|center_vertical"

                        android:text="تهران  "
                        android:textColor="#000"
                        android:textSize="@dimen/_12dp_txtSize" />

                </LinearLayout>
                <!-- ///////////////////////////////////////////////////// -->
            </LinearLayout>

            <RelativeLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="5"
                android:layout_marginLeft="9dp"
                android:layout_marginRight="5dp"

                >

                <TextView
                    android:id="@+id/txtPin"
                    android:layout_width="18dp"
                    android:layout_height="18dp"
                    android:layout_alignParentRight="true"
                    android:layout_alignParentTop="true"
                    android:gravity="center"
                    android:text="@string/icon_pin"
                    android:textAppearance="@style/IconFonts"

                    android:textColor="#aaaaaa"
                    android:textSize="12sp"
                    android:visibility="visible" />
                <ImageView
                    android:id="@+id/lblProductrow"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_centerInParent="true"


                    android:adjustViewBounds="true"
                    android:scaleType="fitXY" />
            </RelativeLayout>
        </LinearLayout>
        <!-- ////////////////////////////front bottom///////////////////////////////////// -->

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="4dp"
            android:layout_marginBottom="4dp"
            android:orientation="horizontal"
            android:layout_gravity="center"
            android:background="@color/sorati" >
            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="3"
                android:gravity="left"
                android:layout_marginTop="4dp"
                android:layout_marginBottom="4dp">

                <!--<ImageView
                    android:id="@+id/btnExpand"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:text="&#45;&#45;"
                    android:src="@drawable/a"
                    android:visibility="visible"/>-->
               <TextView
                    android:id="@+id/btnExpand"
                    android:layout_width="0dp"
                    android:layout_weight="0.1"
                    android:layout_height="25dp"
                   android:gravity="center"
                   android:textSize="@dimen/_12dp_txtSize"
                   android:textAppearance="@style/Arrow"
                   android:textColor="#000"
                   android:text="@string/icon_exp_up"
                   />
                <TextView
                    android:id="@+id/txt_economi"
                    android:layout_width="0dp"
                    android:layout_weight="0.2"

                    android:layout_height="match_parent"
                    android:layout_marginLeft="0dp"

                    android:background="@drawable/background_strock_blue_dark_economi"
                    android:gravity="center"
                    android:layout_gravity="center"
                    android:text="اکونومی"
                    android:textColor="#003399"
                    android:textSize="9dp" />

                <TextView
                    android:id="@+id/txttedad"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_weight="0.5"

                    android:gravity="center"
                    android:text="فقط 4 نفر"
                    android:textColor="#0e874e"
                    android:textSize="10dip"
                    android:visibility="visible" />
            </LinearLayout>
            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_gravity="right|center_vertical"
                android:layout_weight="2"
                android:layout_marginTop="4dp"
                android:layout_marginBottom="4dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="#f5eeff"
                    android:gravity="right"
                    android:text="ریال"
                    android:textColor="#0e874e"

                    android:textAppearance="@style/IrsansBold"
                    android:textSize="14dp" />

                <TextView
                    android:id="@+id/lblAdlCost"
                    android:layout_width="120dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:textAppearance="@style/IrsansBold"
                    android:gravity="left"
                    android:text="10,000,000"
                    android:textColor="#000000"
                    android:textSize="15dp"
                    android:textStyle="bold" />
            </LinearLayout>
        </LinearLayout>

    </LinearLayout>

</LinearLayout>