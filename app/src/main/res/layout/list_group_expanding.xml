<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="#ffffff"
    android:orientation="vertical"
    android:padding="8dp" >

    <!--
         <LinearLayout
        style="@style/MyListFrontContentRe"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal" >
    -->

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical" >

        <TextView
            android:id="@+id/TextView01"
            android:layout_width="8dp"
            android:layout_height="10dp"
            android:layout_gravity="right"
            android:layout_marginBottom="1dp"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="1dp"
            android:layout_marginTop="1dp"
            android:background="@drawable/pin_icon_for_items"
            android:gravity="center"
            android:textColor="#FFFFFF"
            android:textSize="12sp" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="60dp"
        android:layout_marginBottom="1dp"
        android:layout_marginLeft="1dp"
        android:layout_marginTop="1dp"
        android:layout_weight="0.02"
        android:orientation="vertical" >

        <LinearLayout
            android:id="@+id/fronttop"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginLeft="1dp"
            android:layout_marginRight="1dp"
            android:background="#ffffff"
            android:gravity="center"
            android:minHeight="40dp"
            android:orientation="horizontal" >

            <!--
             <ImageView 
	        android:layout_width="wrap_content"
	        android:layout_height="match_parent"
	        android:layout_gravity="center_vertical"
	        
	        />
            -->

            <LinearLayout
                android:id="@+id/linearLayoutSevom"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_gravity="right"
                android:layout_marginRight="3dip"
                android:gravity="right"
                android:orientation="vertical" >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" >

                    <!-- <TextView
                        android:id="@+id/textCharter"
                        android:layout_width="46dp"
                        android:layout_height="wrap_content"
                       android:background="#23b574"
                       
                        android:gravity="center"
                        android:text="چارتر"
                        android:textColor="#fff"
                        android:textSize="10dip" /> -->
<!--   -->
                    <TextView
                        android:id="@+id/txt_economi"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="1dp"
                        android:background="#29aae3"
                        android:gravity="center"
                        android:text="اکونومی"
                        android:textColor="#fff"
                        android:textSize="10dip" />
                </LinearLayout>

                <TextView
                    android:id="@+id/txttedad"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginBottom="4dp"
                    android:layout_marginTop="7dp"
                    android:gravity="center"
                    android:text="فقط 4 نفر"
                    android:textColor="#486b6b"
                    android:textSize="10dip"
                    android:visibility="visible" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" >

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="#fff"
                        android:gravity="right"
                        android:text="ریال"
                        android:textColor="#23b574"
                        android:textSize="10dip" />

                    <TextView
                        android:id="@+id/lblAdlCost"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="1dp"
                        android:gravity="left"
                        android:text="10,000,000"
                        android:textColor="#000000"
                        android:textSize="14dip"
                        android:textStyle="bold" />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/linearLayout1"
                android:layout_width="0dip"
                android:layout_height="match_parent"
                android:layout_gravity="right"
                android:layout_marginRight="3dip"
                android:layout_weight="0.69"
                android:gravity="right"
                android:orientation="vertical" >

                <TextView
                    android:id="@+id/lblFlightArrivalTimeR"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="right"
                    android:text="11:50 Am"
                    android:textColor="#003399"
                    android:textSize="14dip" />

                <TextView
                    android:id="@+id/fghfgh"
                    android:layout_width="201dp"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="4dp"
                    android:layout_marginTop="4dp"
                    android:gravity="right"
                    android:text="11:50 AM"
                    android:textColor="#486b6b"
                    android:textSize="12dip"
                    android:visibility="invisible" />

                <TextView
                    android:id="@+id/lblFlightArrivalTimeB"
                    android:layout_width="201dp"
                    android:layout_height="wrap_content"
                    android:gravity="right"
                    android:text="11:50 AM"
                    android:textColor="#003399"
                    android:textSize="14dip" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/linearLayout2"
                android:layout_width="50dip"
                android:layout_height="match_parent"
                android:layout_above="@+id/linearLayout1"
                android:layout_gravity="right"
                android:layout_marginRight="3dip"
                android:layout_weight="0.45"
                android:gravity="right"
                android:orientation="vertical" >

                <TextView
                    android:id="@+id/lblArrivalCityNameFaR"
                    android:layout_width="127dp"
                    android:layout_height="wrap_content"
                    android:gravity="right"
                    android:text="رفت به استانبول"
                    android:textColor="#000"
                    android:textSize="12dip" />

                <TextView
                    android:id="@+id/lblAirline"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="4dp"
                    android:layout_marginTop="4dp"
                    android:gravity="right"
                    android:text="ترکیش "
                    android:textColor="#003399"
                    android:textSize="14dip" />

                <TextView
                    android:id="@+id/lblArrivalCityNameFaB"
                    android:layout_width="127dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="2dp"
                    android:gravity="right"
                    android:text="برگشت به تهران"
                    android:textColor="#000"
                    android:textSize="12dip" />
            </LinearLayout>

            <ImageView
                android:id="@+id/lblProductrow"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_alignParentRight="true"
                android:layout_centerInParent="true"
                android:layout_marginBottom="8dp"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="1dp"
                android:layout_marginTop="2dp"
                android:adjustViewBounds="true"
                android:scaleType="centerCrop" />

        </LinearLayout>
        <!-- ////////////////////////////front bottom///////////////////////////////////// -->
    </LinearLayout>

</LinearLayout>