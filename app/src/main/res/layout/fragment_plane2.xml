<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff" >
    <Button
        android:id="@+id/btntwo"
        android:layout_width="190dip"
        android:layout_height="50dip"

        android:layout_alignParentRight="true"

        android:layout_weight="0.03"
        android:background="@drawable/purple_button_larg"
        android:gravity="center|fill_vertical"
        android:layout_centerInParent="true"
        android:text="رفت و برگشت"
        android:textColor="#fff"
        android:textSize="12sp"
        android:visibility="visible" />

    <Button
        android:id="@+id/btnOne"
        android:layout_width="190dip"
        android:layout_height="50dip"

        android:layout_alignParentLeft="true"

        android:layout_weight="0.03"
        android:layout_centerInParent="true"
        android:background="@drawable/raft_big"
        android:gravity="center|fill_vertical"
        android:text="رفت"
        android:textColor="#808080"
        android:textSize="12sp"
        android:visibility="visible" />
    <!--
        <ScrollView

            android:layout_width="match_parent"
            android:layout_height="match_parent" >-->
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#000000"
        android:orientation="vertical">

        <RelativeLayout
            android:layout_width="fill_parent"
            android:layout_height="291dp"
            android:layout_gravity="center_vertical|center_horizontal"
            android:background="#fff"
            android:gravity="left">

            <LinearLayout
                android:id="@+id/front"
                style="@style/MyListFrontContentRe"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_260"
                android:background="@drawable/banafsh_jahan"
                android:gravity="center"
                android:minHeight="40dp"
                android:orientation="horizontal"
                android:tag="front">


                <TextView
                    android:id="@+id/lblProductAvailable"
                    android:layout_width="32dp"
                    android:layout_height="match_parent"
                    android:layout_above="@+id/txt_gheymat"
                    android:layout_marginBottom="1dp"
                    android:layout_marginLeft="2dp"
                    android:layout_marginRight="2dp"
                    android:layout_marginTop="1dp"
                    android:layout_toRightOf="@+id/txt_emp_dob"
                    android:layout_weight="0.03"
                    android:background="@drawable/rectangle"
                    android:gravity="center"
                    android:textColor="#FFFFFF"
                    android:textSize="19sp"
                    android:textStyle="bold"
                    android:visibility="gone" />

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="208dp"
                    android:layout_marginRight="15dip"
                    android:layout_marginTop="10dp"
                    android:layout_weight="0.09"
                    android:gravity="right"
                    android:orientation="vertical">

                    <LinearLayout
                        android:id="@+id/linearLayout_mabda"
                        android:layout_width="wrap_content"
                        android:layout_height="101dp"
                        android:layout_above="@+id/linearLayout1"
                        android:layout_gravity="right"
                        android:layout_marginRight="3dip"
                        android:layout_marginTop="10dp"
                        android:gravity="right"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/lblProductID"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="2dp"
                            android:gravity="right"
                            android:text="مبدا : "
                            android:textColor="#ffffff"
                            android:textSize="15dip" />

                        <TextView
                            android:id="@+id/tvStart"
                            android:layout_width="506dp"
                            android:layout_height="wrap_content"
                            android:gravity="right"
                            android:text=" تهران "
                            android:textColor="#fff"
                            android:textSize="20dip"
                            android:textAppearance="@style/IrsansBold"
                            />

                        <TextView
                            android:id="@+id/lbl_forudgah_mabda"
                            android:layout_width="501dp"
                            android:layout_height="wrap_content"
                            android:gravity="right"
                            android:text="(فرودگاه بین المللی امام خمینی) "
                            android:textColor="#fff"
                            android:textSize="11dip" />
                    </LinearLayout>
                    <!-- /////////////////////////////////linear layout maghsad////////////////////////////////////////////// -->

                    <LinearLayout
                        android:id="@+id/linearLayout_maghsad"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_above="@+id/linearLayout1"
                        android:layout_gravity="right"
                        android:layout_marginRight="3dip"
                        android:layout_marginTop="1dp"
                        android:layout_weight="0.02"
                        android:gravity="right"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/lblmaghsad"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="2dp"
                            android:gravity="right"
                            android:text="مقصد : "
                            android:textColor="#ffffff"
                            android:textSize="15dip" />

                        <TextView
                            android:id="@+id/tvEnd"
                            android:layout_width="506dp"
                            android:layout_height="wrap_content"
                            android:gravity="right"
                            android:text=" استانبول "
                            android:textColor="#fff"
                            android:textSize="20dip"
                            android:textAppearance="@style/IrsansBold" />

                        <TextView
                            android:id="@+id/lbl_forudgah_maghsad"
                            android:layout_width="501dp"
                            android:layout_height="wrap_content"
                            android:gravity="right"
                            android:text="(فرودگاه بین المللی آتاتورک) "
                            android:textColor="#fff"
                            android:textSize="11dp" />
                    </LinearLayout>
                </LinearLayout>

                <TextView
                    android:id="@+id/txtOption"
                    android:layout_width="32dp"
                    android:layout_height="190dp"
                    android:layout_marginBottom="8dp"
                    android:layout_marginLeft="8dp"
                    android:layout_marginRight="10dp"
                    android:layout_marginTop="14dp"
                    android:background="@drawable/icswitch_big"
                    android:gravity="center"
                    android:text=""
                    android:textColor="#FFFFFF"
                    android:textSize="12sp" />

            </LinearLayout>



        </RelativeLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="110dp"

            android:background="#fff"
            android:orientation="vertical">

            <android.support.v7.widget.CardView
                android:id="@+id/cvDate"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_gravity="center"
                android:layout_marginTop="1dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_marginBottom="2dp"
                android:layout_weight="100"
                android:background="@color/hotel_detail_background">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="25dp"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/tarikh_be"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_marginRight="65dp"
                            android:layout_weight="100"
                            android:background="#fff"
                            android:gravity="right"
                            android:text="تاریخ برگشت "
                            android:textColor="#808080"
                            android:textSize="14dp"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/tarikh_az"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_marginRight="10dp"
                            android:layout_weight="100"
                            android:background="#fff"
                            android:gravity="right"
                            android:text="تاریخ رفت"
                            android:textColor="#808080"
                            android:textSize="14dp"
                            android:textStyle="bold" />

                    </LinearLayout>


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"

                        android:layout_marginTop="10dp"
                        android:orientation="horizontal">

                        <LinearLayout
                            android:id="@+id/linear_picker"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="100"
                            android:orientation="horizontal">

                            <TextView
                                android:id="@+id/tarikh_be_picker"
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:layout_marginBottom="10dp"
                                android:layout_marginLeft="5dp"
                                android:layout_marginRight="10dp"
                                android:gravity="center"
                                android:text="انتخاب کنید"
                                android:textColor="@color/textColorRDark"
                                android:textSize="12dp" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:layout_marginBottom="10dp"
                                android:gravity="center"
                                android:text="@string/icon_calander"
                                android:textAppearance="@style/IconFonts"
                                android:textColor="@color/textColorRDark"
                                android:textSize="25dp" />


                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_marginRight="10dp"
                            android:layout_weight="100"
                            android:gravity="right"
                            android:orientation="horizontal">

                            <TextView
                                android:id="@+id/tarikh_az_picker"
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:layout_marginBottom="10dp"
                                android:layout_marginLeft="5dp"
                                android:layout_marginRight="10dp"
                                android:gravity="center"
                                android:text="انتخاب کنید"
                                android:textColor="@color/textColorRDark"
                                android:textSize="12dp"

                                />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:layout_marginBottom="10dp"
                                android:gravity="center"
                                android:text="@string/icon_calander"
                                android:textAppearance="@style/IconFonts"
                                android:textColor="@color/textColorRDark"
                                android:textSize="25dp" />


                        </LinearLayout>


                    </LinearLayout>


                </LinearLayout>


            </android.support.v7.widget.CardView>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"

            android:background="#fff"
            android:orientation="vertical">


            <android.support.v7.widget.CardView

                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_gravity="center"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_marginTop="7dp"
                android:layout_marginBottom="7dp"
                android:layout_weight="100"
                android:background="@color/hotel_detail_background">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="135dp"

                    android:orientation="vertical">
                    <LinearLayout
                        android:layout_marginTop="4dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="14dp"
                        android:layout_marginLeft="14dp"
                        android:layout_marginRight="10dp">

                        <Button
                            android:id="@+id/btnMinesB"
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:layout_marginBottom="1dp"
                            android:layout_marginLeft="22dp"
                            android:layout_marginRight="20dp"
                            android:layout_marginTop="1dp"
                            android:background="@drawable/background_strock"
                            android:text="-"
                            android:textColor="#000000" />

                        <TextView
                            android:id="@+id/txtCountB"
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:layout_gravity="top"
                            android:text="1"
                            android:textColor="#000000" />

                        <Button
                            android:id="@+id/btnPlusB"
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:layout_margin="1dp"
                            android:background="@drawable/background_strock"
                            android:text="+"
                            android:textColor="#000000" />

                        <TextView
                            android:id="@+id/txtBO"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="1dp"
                            android:gravity="right"
                            android:text="بزرگسال"
                            android:textColor="#000000"
                            android:textSize="11dp" />

                    </LinearLayout>
                    <!-- ///////////////koodak////////////////// -->

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="14dp"
                        android:layout_marginLeft="14dp"
                        android:layout_marginRight="10dp">

                        <Button
                            android:id="@+id/btnMinesK"
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:layout_marginBottom="1dp"
                            android:layout_marginLeft="22dp"
                            android:layout_marginRight="20dp"
                            android:layout_marginTop="1dp"
                            android:background="@drawable/background_strock"
                            android:text="-"
                            android:textColor="#000000" />

                        <TextView
                            android:id="@+id/txtCountK"
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:layout_gravity="top"
                            android:text="0"
                            android:textColor="#000000" />

                        <Button
                            android:id="@+id/btnPlusK"
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:layout_margin="1dp"
                            android:background="@drawable/background_strock"
                            android:text="+"
                            android:textColor="#000000" />

                        <TextView
                            android:id="@+id/txtKO"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="1dp"
                            android:gravity="right"
                            android:text="کودک"
                            android:textColor="#000000"
                            android:textSize="11dp" />

                    </LinearLayout>
                    <!-- //////////////////////nozad/////////////////////// -->

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="1dp"
                        android:layout_marginLeft="14dp"
                        android:layout_marginRight="10dp">

                        <Button
                            android:id="@+id/btnMinesN"
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:layout_marginBottom="1dp"
                            android:layout_marginLeft="22dp"
                            android:layout_marginRight="20dp"
                            android:layout_marginTop="1dp"
                            android:background="@drawable/background_strock"
                            android:text="-"
                            android:textColor="#000000" />

                        <TextView
                            android:id="@+id/txtCountN"
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:layout_gravity="top"
                            android:text="0"
                            android:textColor="#000000" />

                        <Button
                            android:id="@+id/btnPlusN"
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:layout_margin="1dp"
                            android:background="@drawable/background_strock"
                            android:text="+"
                            android:textColor="#000000" />

                        <TextView
                            android:id="@+id/txtNO"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="1dp"
                            android:gravity="right"
                            android:text="نوزاد"
                            android:textColor="#000000"
                            android:textSize="11dp" />

                    </LinearLayout>
                </LinearLayout>
            </android.support.v7.widget.CardView>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <Button
                    android:id="@+id/searchPlan"
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:layout_gravity="center"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:layout_marginTop="2dp"
                    android:background="@drawable/btngravity_big"
                    android:text="جستجو پرواز"
                    android:textColor="#FFFFFF"
                    android:textSize="11dp" />

            </LinearLayout>

        </LinearLayout>

    </LinearLayout>

    <!-- </ScrollView>-->



</RelativeLayout>